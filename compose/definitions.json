{
  "users": [
    {
      "name": "rabbitmq",
      "password": "rabbitmq",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "rabbitmq",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "exchanges": [
    {
      "name": "temperature-processing.temperature-received.v1.e",
      "vhost": "/",
      "type": "fanout",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "queues": [
    {
      "name": "temperature-monitoring.process-temperature.v1.q",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "temperature-monitoring.alerting.v1.q",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "temperature-processing.temperature-received.v1.e",
      "vhost": "/",
      "destination": "temperature-monitoring.process-temperature.v1.q",
      "destination_type": "queue",
      "routing_key": "temperature.received",
      "arguments": {}
    },
    {
      "source": "temperature-processing.temperature-received.v1.e",
      "vhost": "/",
      "destination": "temperature-monitoring.alerting.v1.q",
      "destination_type": "queue",
      "routing_key": "temperature.received",
      "arguments": {}
    }
  ]
}
